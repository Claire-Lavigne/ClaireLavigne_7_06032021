{
  "name": "clairelavigne_7_06032021",
  "version": "1.0.0",
  "description": "",
  "main": "./js/index.js",
  "scripts": {
    "prebuild": "node -e \"require('fs').mkdirSync('build/css/prefixed',{recursive:true})\"",
    "build:sass": "sass scss/style.scss build/css/style.src.css --style=compressed",
    "build:prefix": "postcss build/css/style.src.css --use autoprefixer -o build/css/prefixed/style.css",
    "build": "npm run prebuild && npm run build:sass && npm run build:prefix",
    "watch:sass": "sass --watch scss/style.scss:build/css/style.src.css --style=compressed",
    "watch:prefix": "postcss build/css/style.src.css --use autoprefixer -o build/css/prefixed/style.css --watch",
    "start": "live-server --port=8080 --host=localhost",
    "dev": "npm run build && npm-run-all --parallel watch:sass watch:prefix start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.94.0"
  },
  "browserslist": "last 4 versions"
}
